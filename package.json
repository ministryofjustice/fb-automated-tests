{
  "name": "fb-automated-tests",
  "private": true,
  "version": "1.0.0",
  "description": "Automated tests for a Form Builder form",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && ava --reset-cache && ava -v",
    "test:ci": "mkdir -p test-results/{traces,har,screenshots} && ./node_modules/.bin/ava -v -- --screenshots --trace"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ministryofjustice/eslint-config-fb": "^1.0.7",
    "ava": "^2.0.0",
    "chrome-har": "^0.10.0",
    "esm": "^3.2.25",
    "puppeteer": "^1.17.0"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "devDependencies": {
    "eslint-plugin-ava": "^7.0.0"
  }
}
